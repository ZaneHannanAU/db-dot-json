{
  "name": "db-dot-json",
  "version": "1.0.0",
  "description": "A near fork of @Belphemur/node-json-db with more options and Bower/requireJS compatibility.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "babel lib --out-dir _ && babel db.js --out-file _db.js",
    "prepublish": "npm run build",
    "postpublish": "npm run clean",
    "clean": "rm -r _db.js _"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ZaneHannanAU/db-dot-json.git"
  },
  "keywords": [
    "db",
    "json",
    "fs",
    "bower"
  ],
  "author": "zanehannanau",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/ZaneHannanAU/db-dot-json/issues"
  },
  "homepage": "https://github.com/ZaneHannanAU/db-dot-json#readme",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-env": "^1.4.0"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": true
          }
        }
      ]
    ]
  },
  "dependencies": {
    "mkdirp": "^0.5.1"
  }
}
